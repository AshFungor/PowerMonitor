<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:oxy="http://oxyplot.org/avalonia"
             x:Class="PowerMonitor.models.Plot">
    
    <oxy:Plot x:Name="Plot">
        <oxy:Plot.Axes>
            <oxy:LinearAxis Position="Bottom" Maximum="24" Minimum="0" Title="hour" FontSize="24"></oxy:LinearAxis>
            <oxy:LinearAxis Position="Left" Maximum="100" Minimum="0" Title="effectiveness" FontSize="24"></oxy:LinearAxis>
        </oxy:Plot.Axes>
    </oxy:Plot>
    
</UserControl>
